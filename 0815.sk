on command:
	loop all players:
		if {cmdspy.%uuid of loop-player%} is true:
			if arguments is set:
				if sender is player:
					send "{@prefix} %player%&8: &3/%command% %arguments%" to loop-player
				if sender is console:
					send "{@prefix} Console&8: &3/%command% %arguments%" to loop-player
			else:
				if sender is player:
					send "{@prefix} %player%&8: &3/%command%" to loop-player
				if sender is console:
					send "{@prefix} Console&8: &3/%command%" to loop-player

