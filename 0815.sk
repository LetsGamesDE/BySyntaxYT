options:
	ServerName: DEINSERVER.DE
	ServerTeamspeak: ts.DeinTS.de
	prefix: &9Lobby &7⎟
	Freunde: &9Freunde &7⎟
	Broadcast: &4&LINFO &7» 
	permission: &9Lobby &7⎟ &cDu hast dafür keine Rechte!
	AutoBroadcast1: Deine Message1
	AutoBroadcast2: Deine Message2
	update1: &6Update Text Nr. 1
	update2: &6Update Text Nr. 2
	update3: &6Update Text Nr. 3
	update4: &6Update Text Nr. 4
	JPPPLATE: stone pressure plate
	JPPBLOCK: bedrock block
	JPPSOUND: mob.endermen.portal
	JPPVOLUME: 0.5
	JPPPITCH: 10
	JPPHÖHE: 0.3
	JPPSPEED: 0.4
	preis1: 200
	preis2: 200
	preis3: 200
	preis4: 200
	preis5: 200
	preis6: 200
	preis7: 200
	preis8: 300
	preis9: 300
	preis10: 300

on First join:
	set join message to "&a» &7Der Spieler &a%player% &7hat den Server betreten."
	execute player command "/resetscore"
	clear player's inventory
	teleport player to {spawn}
	set slot 1 of player to wait music disc named "&6&LTeleporter"
	set slot 2 of player to golden apple named "&e&lShop"
	set slot 4 of player to chest named "&c&lEinstellungen"
	set slot 6 of player to nether star named "&b&LLobby Switcher"
	set slot 7 of player to player's skull named "&5&lFreunde"

on join:
	set join message to "&a» &7Der Spieler &a%player% &7hat den Server betreten."
	clear player's inventory
	set level of player to 0
	teleport player to {spawn}
	set slot 1 of player to wait music disc named "&6&LTeleporter"
	set slot 2 of player to book named "&e&lShop"
	set slot 4 of player to chest named "&c&lEinstellungen"
	set slot 6 of player to nether star named "&b&LLobby Switcher"
	set slot 7 of player to player's skull named "&5&lFreunde"
	send "&8&m--------------------------------"
	send ""
	send "     &b➤ &7Es gibt Neue Updates!"
	send "     &b➤ &6/Update"
	send "     &b➤ &7Viel Spaß beim Spielen!"
	send ""
	send "&8&m--------------------------------"
	send player title "&9Willkommen" with subtitle "&b%player%" for 5 seconds
	set {lobby.%player%.build} to false

on quit:
	set quit message to "&a» &7Der Spieler &a%player% &7hat den Server verlassen."

on weather change to rain or thunder:
	cancel event

on break:
	if {lobby.%player%.build} is false:
		cancel event
		
on craft:
