command /report [<player>]:
	trigger:
		if arg 1 is not set:
			send "{@prefix} Benutze &e/report <Spielername>"
		if "%arg 1%" is "%player%":
			send "{@prefix} Du kannst dich nicht selbst reporten"
			stop
		if arg 1 is set:
			wait 1 tick
			open chest with 3 rows named "&cReport &e%arg 1%" to player
			format slot 10 of player with iron sword named "&eHacking" to close then run [execute player command "sreport %arg 1% Hacking"]
			format slot 11 of player with paper named "&eChat" to close then run [execute player command "sreport %arg 1% Chat"]
			format slot 12 of player with player head named "&eSkin" to close then run [execute player command "sreport %arg 1% Skin"]
			format slot 13 of player with name tag named "&eName" to close then run [execute player command "sreport %arg 1% Name"]
			format slot 14 of player with pig spawn egg named "&eHaustiere" to close then run [execute player command "sreport %arg 1% Haustiere"]
			format slot 15 of player with iron chestplate named "&eClan" to close then run [execute player command "sreport %arg 1% Clan"]
			format slot 16 of player with sign named "&eStatus" to close then run [execute player command "sreport %arg 1% Status"]

command /sreport [<player>] [<text>]:
	trigger:
		if arg 1 is not set:
			send "{@prefix} Benutze &e/report <Spielername> <Grund>"
			if arg 2 is not set:
				send "{@prefix} Benutze &e/report <Spielername> <Grund>"
		if "%arg 1%" is "%player%":
			send "{@prefix} Du kannst dich nicht selbst reporten"
		else:
			send "{@prefix} Du hast &e%arg 1% &7wegen &4%arg 2% &7gemeldet" to sender
			play "CREEPER_HISS" to player
			loop all players:
				if loop-player has permission "lobby.reports":
					send "{@prefix} &e%player% &7hat &c%arg 1% &7wegen &4%arg 2% &7gemeldet" to loop-player
					stop

