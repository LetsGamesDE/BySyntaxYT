command /friend [<text>] [<player>] [<text>]:
	aliases: f, freund, freunde
	trigger:
		if arg 1 is not set:
			send "{@Freunde} Hilfe%nl%&e/friend add <Spielername>%nl%&e/friend remove <Spielername>%nl%&e/friend list%nl%&e/friend accept <Spielername>%nl%&e/friend deny <Spielername>"
		if arg 1 is "add":
			loop {friend.list.%player%::*}:
				if arg 2 is loop-value:
					send "{@Freunde} &7Du bist bereits mit &c%arg 2% &7befreundet"
					stop
			if arg 2 is player:
				send "{@Freunde} &7Du kannst dir keine Anfrage stellen"
				stop
			set {friend.request.%arg 2%} to player
			set {friend.request.%player%} to arg 2
			send "{@Freunde} &7Du hast &a%arg 2% &7eine Anfrage gestellt"
			send "{@Freunde} &7Du hast von &a%player% &7eine Anfrage bekommen" to arg 2
		if arg 1 is "accept":
			if {friend.request.%player%} is arg 2:
				if {friend.request.%arg 2%} is player:
					set {friend.request.%player%} to 0
					set {friend.request.%arg 2%} to 0
					add player to {friend.list.%arg 2%::*}
					add arg 2 to {friend.list.%player%::*}
					send "{@Freunde} &7Du bist nun mit &a%arg 2% &7befreundet"
					send "{@Freunde} &7Du bist nun mit &a%player% &7befreundet" to arg 2
		if arg 1 is "deny":	
			if {friend.request.%player%} is arg 2:
				if {friend.request.%arg 2%} is player:
					set {friend.request.%player%} to 0
					set {friend.request.%arg 2%} to 0
					send "{@Freunde} &7Du hast die Anfrage von &c%arg 2% &7abgelehnt"
		if arg 1 is "list":
			wait 0.1 second
			set {friend.count.%player%} to 9
			open chest with 6 rows named "&5&lFreunde Menü" to player
			wait 1 tick
			loop {friend.list.%player%::*}:
				if loop-value is online:
					format slot {friend.count.%player%} of player with loop-value's skull named "&3%loop-value%" with lore " &7Aktuell&8: &aOnline" to close then run [execute player command "/playeron"]
					add 1 to {friend.count.%player%}
				if loop-value is offline:
					format slot {friend.count.%player%} of player with player's skull named "&7%loop-value%" with lore " &7Aktuell&8: &cOffline" to close then run [execute player command "/playeroff"]
					add 1 to {friend.count.%player%}					
		if arg 1 is "remove":
			loop {friend.list.%player%::*}:
				if arg 3 is loop-value:
					remove arg 3 from {friend.list.%player%::*}
					send "&8[&2Freunde&8] &7Du bist nun nicht mehr mit &c%arg 3% &7befreundet"
			loop {friend.list.%arg 3%::*}:
				if player is loop-value:
					remove player from {friend.list.%arg 3%::*}
			loop {friend.list.%player%::*}:
				if arg 2 is loop-value:
					remove arg 2 from {friend.list.%player%::*}
					send "&8[&2Freunde&8] &7Du bist nun nicht mehr mit &c%arg 2% &7befreundet"
			loop {friend.list.%arg 2%::*}:
				if player is loop-value:
					remove player from {friend.list.%arg 2%::*}
		format slot 0 of player with gray stained glass named "&c&l" to be unstealable
		format slot 1 of player with gray stained glass named "&c&l" to be unstealable
		format slot 2 of player with gray stained glass named "&c&l" to be unstealable
		format slot 3 of player with gray stained glass named "&c&l" to be unstealable
		format slot 4 of player with paper named "&eFreunde" to be unstealable
		format slot 5 of player with gray stained glass named "&c&l" to be unstealable
		format slot 6 of player with gray stained glass named "&c&l" to be unstealable
		format slot 7 of player with gray stained glass named "&c&l" to be unstealable
		format slot 8 of player with gray stained glass named "&c&l" to be unstealable
		format slot 45 of player with gray stained glass named "&c&l" to be unstealable
		format slot 46 of player with gray stained glass named "&c&l" to be unstealable
		format slot 47 of player with gray stained glass named "&c&l" to be unstealable
		format slot 48 of player with gray stained glass named "&c&l" to be unstealable
		format slot 49 of player with paper named "&c&lStatus: &c%{status.%uuid of player%}%" to be unstealable
		format slot 50 of player with gray stained glass named "&c&l" to be unstealable
		format slot 51 of player with gray stained glass named "&c&l" to be unstealable
		format slot 52 of player with gray stained glass named "&c&l" to be unstealable
		format slot 53 of player with gray stained glass named "&c&l" to be unstealable

command /setup:
	permission: lobby.setup
	trigger:
		wait 3 ticks
		open chest with 2 rows named "&9Lobby&f-&9Setup" to player
		wait 1 tick
		format slot 0 of player with bed item named "&cBedwars" to close then run [make player execute command "/setbedwars"]
		format slot 1 of player with grass named "&9SkyWars" to close then run [make player execute command "/setskywars"]
		format slot 2 of player with golden sword named "&6QSG" to close then run [make player execute command "/setqsg"]
		format slot 3 of player with wooden axe named "&eGunGame" to close then run [make player execute command "/setgungame"]
		format slot 4 of player with stick named "&aKnockbackFFA" to close then run [make player execute command "/setknockbackffa"]
		format slot 5 of player with diamond chestplate named "&dFFA" to close then run [make player execute command "/setffa"]
		format slot 6 of player with nether star named "&bSpawn" to close then run [make player execute command "/setspawn"]
		wait 1 tick
		format slot 9 of player with light green clay named "&aLobby-1" to close then run [make player execute command "/setlobby-1"]
		format slot 10 of player with light green clay named "&aLobby-2" to close then run [make player execute command "/setlobby-2"]
		format slot 11 of player with light green clay named "&6Premium-Lobby" to close then run [make player execute command "/setplobby-1"]

every 1 second:
	if {lobby.scoreboard} is true:
		loop all players:
			wipe loop-player's sidebar
			set name of sidebar of loop-player to "&f&L{@ServerName}"
			set score "&a " in sidebar of loop-player to 11
			set score "&fRang&7:" in sidebar of loop-player to 10
			set score "%{rang.%loop-player%}% " in sidebar of loop-player to 9
			set score "&f " in sidebar of loop-player to 8		
			set score "&fCoins&7:" in sidebar of loop-player to 7
			set score "&e%{lobby.%loop-player%.coins}%" in sidebar of loop-player to 6
			set score "&c" in sidebar of loop-player to 5
			set score "&fSpielzeit&7:" in sidebar of loop-player to 4
			set score "&6%{lobby.%loop-player%.onlinetime}%h" in sidebar of loop-player to 3
			set score "&f" in sidebar of loop-player to 2
			set score "&fTeamspeak&7:" in sidebar of loop-player to 1
			set score "&b{@ServerTeamspeak}" in sidebar of loop-player to 0

every 1 second:
	if {lobby.scoreboard} is false:
		loop all players:
			wipe loop-player's sidebar

command /scoreboard <text>:
	permission: lobby.scoreboard
	usage: &c/scoreboard on/off
	trigger:
		if arg 1 is "on":
			if {lobby.scoreboard} is false:
				set {lobby.scoreboard} to true
				send "{@prefix} &aDu hast das Scoreboard an gemacht."
				send "{@prefix} &6Bitte beachte das wenn das Scoreboard an ist das dieses zu Bugs führen kann."
		if arg 1 is "off":
			set {lobby.scoreboard} to false
			send "{@prefix} &aDu hast das Scoreboard aus gemacht."

every real hour:
	loop all players:
		add 1 to {lobby.%loop-player%.onlinetime}

command /ontime:
	trigger:
		send "{@prefix} &7Du hast eine &aOnlinezeit &7von &a%{lobby.%player%.onlinetime}% &7Stunden."

command /adminontime <text> <player> <number>:
	permission: lobby.ontime
	trigger:
		if arg 1 is "add":
			if arg 3 is set:
				if arg 3 is set:
					add arg 3 to {lobby.%arg 2%.onlinetime}
					send "{@prefix} &7Du hast &a%arg 2% &6%arg 3% &7Stunde Zeit hinzugefügt!"
		if arg 1 is "remove":
			if arg 2 is set:
				if arg 2 is set:
					remove arg 3 from {lobby.%arg 2%.onlinetime}
					send "{@prefix} &7Du hast &a%arg 2% &6%arg 3% &7Stunde Zeit entfernt!"

every 600 seconds:
	wait 1 tick
	broadcast "&8&m------------&a {@ServerName} &8&m------------"
	broadcast ""
	broadcast "{@AutoBroadcast1}"
	broadcast ""
	broadcast "&8&m------------&a {@ServerName} &8&m------------"
	wait 1 tick
	wait 300 seconds
	wait 1 tick
	broadcast "&8&m------------&a {@ServerName} &8&m------------"
	broadcast ""
	broadcast "{@AutoBroadcast2}"
	broadcast ""
	broadcast "&8&m------------&a {@ServerName} &8&m------------"

