command /all:
	trigger:
		wait 3 ticks
		reveal all players from player
		send "{@prefix} &7Du siehst nun &aalle Spieler!"

command /nurT:
	trigger:
		wait 3 ticks
		hide all players from player
		loop all players:
			if loop-player have permission "lobby.bypasshider":
				reveal loop-player from player
		send "{@prefix} &7Du siehst nun &5nur noch Teammitglieder!"

command /non:
	trigger:
		wait 3 ticks
		hide all players from player
		send "{@prefix} &7Du siehst nun &ckeine Spieler mehr!"

