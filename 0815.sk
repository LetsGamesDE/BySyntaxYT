command /fly:
	Executable by: players
	Permission: lobby.fly
	permission message: {@permission}
	Trigger:
		If {fly.%player%} is not set:
			Send "{@prefix} &7Du kannst nun Fliegen!" to player
			set player's flight mode to true
			Set {fly.%player%} to true
			Stop
		Send "{@prefix} &7Du kannst nun nicht mehr Fliegen!" to player
		set player's flight mode to false
		Delete {fly.%player%}
		Stop
command /gm <text>:
	permission: lobby.gm
	permission message: {@permission}
	trigger:
		if player has permission "Lobby.gm":
			if arg 1 is "0" or "survival":
				set player's gamemode to survival
				send "{@prefix} &7Du bist nun im &cSurvivalmodus"
			
			if arg 1 is "1" or "creative":
				set player's gamemode to creative
				send "{@prefix} &7Du bist nun im &cKreativmodus"
			
			if arg 1 is "2" or "adventure":
				set player's gamemode to adventure
				send "{@prefix} &7Du bist nun im &cAdventuremodus"
			
			if arg 1 is "3" or "spectator":
				execute command "/gamemode 3 %player%"
				send "{@prefix} &7Du bist nun im &cSpectatormodus"

